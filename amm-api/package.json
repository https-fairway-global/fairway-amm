{
  "name": "@fairway/amm-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "author": "Heikki Ruhanen/Fairway",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "prepack": "yarn build",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.10",
    "@types/pino": "^7.0.5",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fairway/signature-registry-contract": "link:../signature-registry-contract",
    "@identity-amm/amm-contract": "link:../amm-contract",
    "@midnight-ntwrk/compact-runtime": "^0.7.0",
    "@midnight-ntwrk/midnight-js-contracts": "0.2.5",
    "@midnight-ntwrk/midnight-js-types": "0.2.5",
    "@midnight-ntwrk/midnight-js-utils": "0.2.5",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pino": "^8.16.0",
    "rxjs": "^7.8.0"
  }
}
